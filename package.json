{
	"name": "ww-test",
	"version": "1.0.1",
	"private": true,
	"scripts": {
		"dev": "env-cmd -e target_preview nuxt",
		"spa": "env-cmd -e target_preview,spa nuxt",
		"build": "NUXT_ENV_STORYBLOK_PREVIEW='true' NUXT_ENV_IS_SPA='true' nuxt build",
		"local:build:preview": "env-cmd -e target_preview,spa nuxt build",
		"local:build:prod": "env-cmd -e target_prod,spa nuxt build",
		"generate": "NUXT_ENV_STORYBLOK_PREVIEW='false' nuxt generate",
		"local:generate:preview": "env-cmd -e target_preview nuxt generate",
		"local:generate:prod": "env-cmd -e target_prod nuxt generate",
		"start": "nuxt start",
		"----------- MISC ----------": "",
		"lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
		"lint": "npm run lint:js",
		"test": "jest",
		"generate:splashes": "node splashes-generate.js",
		"story:bak": "env-cmd -e storyblok npx storyblok-backup --components --stories"
	},
	"lint-staged": {
		"*.{js,vue}": "eslint"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@fontsource/inter": "4.5.0",
		"@fontsource/titillium-web": "4.5.0",
		"@marvr/storyblok-rich-text-vue-renderer": "2.0.1",
		"@nuxtjs/axios": "5.13.6",
		"@nuxtjs/proxy": "2.1.0",
		"@vue/composition-api": "1.1.2",
		"core-js": "3.16.2",
		"lg-fullscreen.js": "1.2.0",
		"lg-thumbnail.js": "1.2.0",
		"lg-zoom.js": "1.3.0",
		"lightgallery.js": "1.4.0",
		"nuxt": "2.15.8",
		"nuxt-client-init-module": "0.3.0",
		"nuxt-stripejs": "1.1.6",
		"smoothscroll-polyfill": "0.4.4",
		"storyblok-nuxt": "2.0.1",
		"vue-horizontal": "0.8.10",
		"vue-rellax": "0.2.0",
		"vue2-flip-countdown": "0.11.1"
	},
	"devDependencies": {
		"@aceforth/nuxt-netlify": "1.1.0",
		"@babel/eslint-parser": "7.15.0",
		"@commitlint/cli": "13.1.0",
		"@commitlint/config-conventional": "13.1.0",
		"@epegzz/sass-vars-loader": "6.1.0",
		"@nuxtjs/eslint-config": "6.0.1",
		"@nuxtjs/eslint-module": "3.0.2",
		"@nuxtjs/html-validator": "0.5.0",
		"@nuxtjs/netlify-files": "1.2.0",
		"@nuxtjs/pwa": "3.3.5",
		"@nuxtjs/vuetify": "1.12.1",
		"@vue/test-utils": "1.2.2",
		"babel-core": "7.0.0-bridge.0",
		"babel-jest": "26.6.3",
		"env-cmd": "10.1.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-nuxt": "2.0.0",
		"eslint-plugin-prettier": "3.4.1",
		"eslint-plugin-vue": "^7.16.0",
		"husky": "4.3.8",
		"jest": "26.6.3",
		"lint-staged": "11.1.2",
		"node-html-to-image": "3.2.0",
		"nuxt-canonical": "1.0.6",
		"prettier": "2.3.2",
		"pwa-asset-generator": "4.3.6",
		"sass": "1.32.13",
		"storyblok-migrate": "0.3.4",
		"trianglify": "4.1.1",
		"vue-feather-icons": "5.1.0",
		"vue-jest": "4.0.1",
		"vuetify": "2.5.8"
	}
}
