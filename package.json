{
	"name": "ww-test",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "env-cmd -e target_preview nuxt",
		"spa": "env-cmd -e target_preview,spa nuxt",
		"build": "NUXT_ENV_STORYBLOK_PREVIEW='true' NUXT_ENV_IS_SPA='true' nuxt build",
		"local:build:preview": "env-cmd -e target_preview,spa nuxt build",
		"local:build:prod": "env-cmd -e target_prod,spa nuxt build",
		"generate": "NUXT_ENV_STORYBLOK_PREVIEW='false' nuxt generate",
		"local:generate:preview": "env-cmd -e target_preview nuxt generate",
		"local:generate:prod": "env-cmd -e target_prod nuxt generate",
		"start": "nuxt start",
		"----------- MISC ----------": "",
		"lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
		"lint": "npm run lint:js",
		"test": "jest",
		"story:bak": "env-cmd -e storyblok npx storyblok-backup --components --stories"
	},
	"lint-staged": {
		"*.{js,vue}": "eslint"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@marvr/storyblok-rich-text-vue-renderer": "2.0.1",
		"@nuxtjs/axios": "5.12.5",
		"@nuxtjs/proxy": "2.1.0",
		"@vue/composition-api": "1.0.0-rc.1",
		"access-log": "0.4.1",
		"core-js": "3.8.3",
		"lg-fullscreen.js": "1.2.0",
		"lg-thumbnail.js": "1.2.0",
		"lg-zoom.js": "1.3.0",
		"lightgallery.js": "1.4.0",
		"nuxt": "2.14.7",
		"storyblok-nuxt": "1.3.0",
		"vue2-flip-countdown": "0.11.1"
	},
	"devDependencies": {
		"@aceforth/nuxt-netlify": "1.1.0",
		"@commitlint/cli": "11.0.0",
		"@commitlint/config-conventional": "11.0.0",
		"@epegzz/sass-vars-loader": "6.0.0",
		"@nuxtjs/eslint-config": "5.0.0",
		"@nuxtjs/eslint-module": "3.0.2",
		"@nuxtjs/html-validator": "0.1.6",
		"@nuxtjs/netlify-files": "1.2.0",
		"@nuxtjs/pwa": "3.3.4",
		"@nuxtjs/vuetify": "1.11.3",
		"@vue/test-utils": "1.1.2",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "10.1.0",
		"babel-jest": "26.6.3",
		"env-cmd": "10.1.0",
		"eslint": "7.18.0",
		"eslint-config-prettier": "7.2.0",
		"eslint-plugin-nuxt": "2.0.0",
		"eslint-plugin-prettier": "3.3.1",
		"husky": "4.3.8",
		"jest": "26.6.3",
		"lint-staged": "10.5.3",
		"node-html-to-image": "3.1.0",
		"npm-run-all": "4.1.5",
		"nuxt-canonical": "1.0.6",
		"prettier": "2.2.1",
		"storyblok-migrate": "0.3.4",
		"vue-feather-icons": "5.1.0",
		"vue-jest": "4.0.0-rc.1",
		"vuetify": "2.4.3"
	}
}
